<Project Sdk="Microsoft.NET.Sdk">
	<Import Project="$(SolutionDir)CommonSettingsApplication.xml" />
  <PropertyGroup>
	  <AssemblyName>protogen</AssemblyName>
	  <Product>Protogen</Product>
	  <Description>Generates JSON Web protocol service and client classes.</Description>
	  <Copyright>Â© 2015-2021</Copyright>
	  <Title>Protocol</Title>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Goedel.Tool.Protogen\Goedel.Tool.Protogen.csproj" />
    <ProjectReference Include="..\..\Libraries\Goedel.Tool.Core\Goedel.Tool.Core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Protogen.command">
      <Generator></Generator>
    </None>
  </ItemGroup>

  <Target Name="PreBuild" BeforeTargets="PreBuildEvent">
