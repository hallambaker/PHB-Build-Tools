$TTL 600
phbooth.com.      IN      SOA     dns1.phbooth.com. hallam.gmail.com. (
                        2020071201       ; serial, todays date + todays serial 
                        3600              ; refresh, seconds
                        1800              ; retry, seconds
                        3600000              ; expire, seconds
                        600 )            ; minimum, seconds

; Additional name servers
phbooth.com.     IN      NS      dns1.phbooth.com.
dns1.phbooth.com.    A 178.62.79.124 
phbooth.com.     IN      NS      dns2.phbooth.com.
dns2.phbooth.com.    A 139.59.200.120 


phbooth.com.    IN     CAA 0 issue "comodoca.com"
phbooth.com.    IN     CAA 0 issue "letsencrypt.org"
phbooth.com.    IN     CAA 0 issuewild  ";"
phbooth.com.    IN     CAA 0 iodef "mailto:hostmaster@hallambaker.com"

; Hardcoded A records

; For now, forward all mail to a forwarder on the authoritative
phbooth.com.       IN      MX       1 smtp1.hallambaker.com.


