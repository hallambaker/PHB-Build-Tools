; PATH=/etc/bind/zones/db.bitkitten.org
; 
; DNS Zone file for bitkitten.org using DNS setup Default
;
; This file is automatically generated CHANGES WILL BE OVERWRITTEN!
;
$TTL 600
example.com.      IN      SOA     dns1.example.com. hallam.gmail.com. (
                        2021070601       ; serial, todays date + todays serial 
                        3600              ; refresh, seconds
                        1800              ; retry, seconds
                        3600000              ; expire, seconds
                        600 )            ; minimum, seconds

; Additional name servers
example.com.     IN      NS      dns1.example.com.
dns1.example.com.    A 178.62.79.124 
example.com.     IN      NS      dns2.example.com.
dns2.example.com.    A 139.59.200.120 


; Fallback discovery address for the service
mmm.example.com. IN A 127.0.0.1

; Host host1
host1.example.com. IN A 127.0.0.1

; Service bindings
_mmm._tcp.example.com. SRV 1 1 15099 host1.example.com.

