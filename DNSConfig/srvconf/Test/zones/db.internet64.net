$TTL 600
internet64.net.      IN      SOA     dns1.internet64.net. hallam.gmail.com. (
                        2025012401       ; serial, todays date + todays serial 
                        3600              ; refresh, seconds
                        1800              ; retry, seconds
                        3600000              ; expire, seconds
                        600 )            ; minimum, seconds

; Additional name servers
internet64.net.     IN      NS      dns1.internet64.net.
dns1.internet64.net.    A 178.62.79.124 
internet64.net.     IN      NS      dns2.internet64.net.
dns2.internet64.net.    A 139.59.200.120 


internet64.net.    IN     CAA 0 issue "comodoca.com"
internet64.net.    IN     CAA 0 issue "letsencrypt.org"
internet64.net.    IN     CAA 0 issuewild  ";"
internet64.net.    IN     CAA 0 iodef "mailto:hostmaster@hallambaker.com"

; Hardcoded A records

; default mail
internet64.net.       IN      MX       10 mx01.ionos.com.
internet64.net.       IN      MX       10 mx00.ionos.com.
internet64.net. TXT "v=spf1 redirect=_spf.google.com"



; Host host1.mathmesh.com 178.62.79.124 
internet64.net.    A 178.62.79.124 
www.internet64.net.    A 178.62.79.124 
http.internet64.net.    A 178.62.79.124
https.internet64.net.    A 178.62.79.124 
_http._tcp.internet64.net.  IN    SRV 1 1 80 host1.mathmesh.com.
_https._tcp.internet64.net.   IN   SRV 1 1 443 host1.mathmesh.com.


