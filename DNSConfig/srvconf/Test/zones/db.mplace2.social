$TTL 600
mplace2.social.      IN      SOA     dns1.mplace2.social. hallam.gmail.com. (
                        2024121001       ; serial, todays date + todays serial 
                        3600              ; refresh, seconds
                        1800              ; retry, seconds
                        3600000              ; expire, seconds
                        600 )            ; minimum, seconds

; Additional name servers
mplace2.social.     IN      NS      dns1.mplace2.social.
dns1.mplace2.social.    A 178.62.79.124 
mplace2.social.     IN      NS      dns2.mplace2.social.
dns2.mplace2.social.    A 139.59.200.120 


mplace2.social.    IN     CAA 0 issue "comodoca.com"
mplace2.social.    IN     CAA 0 issue "letsencrypt.org"
mplace2.social.    IN     CAA 0 issuewild  ";"
mplace2.social.    IN     CAA 0 iodef "mailto:hostmaster@hallambaker.com"

; Hardcoded A records

; For now, forward all mail to a forwarder on the authoritative
mplace2.social.       IN      MX       1 smtp1.hallambaker.com.


; Host host1.mathmesh.com 178.62.79.124 
mplace2.social.    A 178.62.79.124 
www.mplace2.social.    A 178.62.79.124 
http.mplace2.social.    A 178.62.79.124
https.mplace2.social.    A 178.62.79.124 
_http._tcp.mplace2.social.  IN    SRV 1 1 80 host1.mathmesh.com.
_https._tcp.mplace2.social.   IN   SRV 1 1 443 host1.mathmesh.com.



